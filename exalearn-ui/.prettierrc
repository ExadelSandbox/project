{
    "printWidth": 120,
    "singleQuote": true,
    "endOfLine": "auto",
    "trailingComma": "none",
    "arrowParens": "avoid",
    
    
    "overrides": [
        {
            "files": "*.component.html",
            "options": {
                "parser": "angular"
            }
        },
        {
            "files": "*.html",
            "options": {
                "parser": "html"
            }
        }
    ]
}